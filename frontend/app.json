{
  "expo": {
    "name": "StudioBuda ArtHub",
    "slug": "studiobuda-arthub",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "studiobuda-arthub",
    "splash": {
      "backgroundColor": "#5D3587"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studiobuda.arthub",
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.YOUR_GOOGLE_CLIENT_ID"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5D3587"
      },
      "package": "com.studiobuda.arthub",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "7446fbd0-f3ee-4957-8257-191a5c737112"
      }
    }
  }
}
